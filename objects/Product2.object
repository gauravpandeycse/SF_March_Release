    <validationRules>
        <fullName>CannotCreateTempsLinkedToInactivePrincip</fullName>
        <active>true</active>
        <description>Preventing the CREATION of new temporary products linked to inactive Principals (accounts)</description>
        <errorConditionFormula>AND ( 
$Setup.Global_Bypass_Processes_Settings__c.Run_Validation_Rules__c =True, 
NOT ($Permission.LockedProductBypass ),
ISNEW(),
RecordType.DeveloperName = &apos;TempProduct&apos;,
TEXT( Principal__r.Status__c) = &apos;Not Active&apos; 
)</errorConditionFormula>
        <errorDisplayField>Principal__c</errorDisplayField>
        <errorMessage>Please select an active principal</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LockedProductReadonly</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
$Setup.Global_Bypass_Processes_Settings__c.Run_Validation_Rules__c, 
NOT ($Permission.LockedProductBypass ),
LockedForIntegration__c, 
OR ( 
ISCHANGED ( FullSegmentsNumber__c), 
ISCHANGED ( Item__c), 
ISCHANGED ( Name), 
ISCHANGED ( ItemStatus__c), 
ISCHANGED ( PackSize__c), 
ISCHANGED ( Type__c), 
ISCHANGED ( SupplierItemNumber__c), 
ISCHANGED ( PackageDescription__c), 
ISCHANGED ( CountryofOrigin__c), 
ISCHANGED ( TradeName__c), 
ISCHANGED ( CASNumber__c), 
ISCHANGED ( CompanySampleStore__c), 
ISCHANGED ( CentralSampleStore__c), 
ISCHANGED ( AllowedCountries__c), 
ISCHANGED ( Principal__c), 
ISCHANGED ( BgcPlasticsId__c), 
ISCHANGED ( BgcFoodId__c), 
ISCHANGED ( BgcPersonalCareId__c), 
ISCHANGED ( BgcFoodId__c), 
ISCHANGED ( BgcLubricantsId__c), 
ISCHANGED ( BgcSynthesisId__c), 
ISCHANGED ( BgcHomeCareId__c), 
ISCHANGED ( BgcOtherId__c), 
ISCHANGED ( BGCPharmaceuticalsId__c ), 
ISCHANGED ( BgcCoatingsId__c), 
ISCHANGED ( Segment4__c), 
ISCHANGED ( UnitOfMeasure__c),
ISCHANGED ( Comments__c )
) 
)</errorConditionFormula>
        <errorMessage>You&apos;re not allowed to change products in Salesforce.  You can edit this information in the Product Hub or in your local product management tool.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PreventNewRecord</fullName>
        <active>true</active>
        <description>Used to prevent users from creating records from the &apos;Recently Viewed&apos; list view. This is done by identifying the special record type &apos;PreventNewRecord&apos;.</description>
        <errorConditionFormula>AND ( 
   $Setup.Global_Bypass_Processes_Settings__c.Run_Validation_Rules__c
   , RecordTypeName__c = &apos;PreventNewRecord&apos; 
)</errorConditionFormula>
        <errorMessage>You are not allowed to create products from here. Please navigate to a quote and create the product from there.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProductNameLengthMax40Chars</fullName>
        <active>true</active>
        <description>Based on OPH rule, Name can only contain 100 chars.</description>
        <errorConditionFormula>AND ( 
$Setup.Global_Bypass_Processes_Settings__c.Run_Validation_Rules__c, 
LEN(Name) &gt; 100 
)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Data value too large (max length=100)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>itemStatusStateModel</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
	OR ( 
		ISPICKVAL (ItemStatus__c, &apos;02.Temp Pre Sales&apos;),
		ISPICKVAL (ItemStatus__c, &apos;03.Active Pre Sales&apos;)
	),
	OR (
		ISPICKVAL( PRIORVALUE(ItemStatus__c), &apos;04.Active Sales&apos;),
		ISPICKVAL( PRIORVALUE(ItemStatus__c), &apos;05.Phase Out&apos;),
		ISPICKVAL( PRIORVALUE(ItemStatus__c), &apos;07.Obsolete&apos;)
	)
)</errorConditionFormula>
        <errorMessage>It&apos;s not allowed to change a Sales product back to a Temporary product.</errorMessage>
    </validationRules>
