    <fields>
        <fullName>OpportunityErrorReason__c</fullName>
        <externalId>false</externalId>
        <formula>IF (
				AND (
				    TEXT(CurrencyIsoCode) &lt;&gt;  TEXT(SBQQ__PrimaryQuote__r.CurrencyIsoCode)
								, NOT (ISBLANK ( SBQQ__PrimaryQuote__r.Id ) )
				)
				, &apos;Opportunity and Primary Quote Currency mismatch&apos;, &apos;|&apos;
)
+ 
IF (
				AND (
			     Pricebook2Id &lt;&gt; SBQQ__PrimaryQuote__r.SBQQ__PriceBook__c
								, NOT (ISBLANK ( SBQQ__PrimaryQuote__r.Id ) )
				)
				, &apos;Opportunity and Primary Quote Pricebook mismatch&apos;, &apos;|&apos;
)
+
IF ( ISBLANK (SBQQ__PrimaryQuote__r.Id), &apos;Opportunity has no Primary quote&apos;, &apos;&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Opportunity Error Reason</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>OpportunityError__c</fullName>
        <description>Checks whether the Opportunity has potential issues. Sibbling field OpportunityErrorReason__c
- Currency &lt;&gt; the Primary Quote&apos;s Currency</description>
        <externalId>false</externalId>
        <formula>OR (
				AND (
								TEXT(CurrencyIsoCode) &lt;&gt;  TEXT(SBQQ__PrimaryQuote__r.CurrencyIsoCode)
								, NOT ISBLANK (SBQQ__PrimaryQuote__r.Id)
				)
    , AND (
 								Pricebook2Id &lt;&gt;  SBQQ__PrimaryQuote__r.SBQQ__PriceBook__c
								, NOT ISBLANK (SBQQ__PrimaryQuote__r.Id)
				)
				, ISBLANK (SBQQ__PrimaryQuote__r.Id)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Opportunity Error</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
