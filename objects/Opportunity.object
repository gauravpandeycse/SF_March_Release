    <validationRules>
        <fullName>CannotChangeLob1</fullName>
        <active>true</active>
        <description>LOB1 cannot be changed, once it&apos;s set. Only allowed by custom permission. Changing the LOB1 has impact on custom sharing. Initially shared Opportunities and child Quotes based on LOB1 will not be automatically re-calculated.</description>
        <errorConditionFormula>IF (
   AND (
				 $Setup.Global_Bypass_Processes_Settings__c.Run_Validation_Rules__c
     , NOT ( $Permission.LockedOpportunityLob1Bypass )
					, ISCHANGED ( LOB1__c )
					, NOT ( ISBLANK ( TEXT ( PRIORVALUE ( LOB1__c ) ) ) )
    )
				, true
    , false
)</errorConditionFormula>
        <errorMessage>After creating an Opportunity you&apos;re only allowed to change the LOB2 and LOB3.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CannotCreateOppsLinkedToInactiveAccounts</fullName>
        <active>true</active>
        <description>Preventing the CREATION of new Opps linked to inactive accounts.</description>
        <errorConditionFormula>AND (
$Setup.Global_Bypass_Processes_Settings__c.Run_Validation_Rules__c =True,
ISNEW(),
TEXT(Account.Status__c) = &apos;Not Active&apos;
)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>Please select an active account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ClosedWonOnlyOnSoldTo</fullName>
        <active>true</active>
        <description>Opportunities can only be set to &apos;Closed Won&apos; or &apos;Closing&apos; on Sold To Accounts</description>
        <errorConditionFormula>$Setup.Global_Bypass_Processes_Settings__c.Run_Validation_Rules__c =True &amp;&amp;
OR(ISPICKVAL(StageName , &apos;Won&apos;),ISPICKVAL(StageName , &apos;Closing&apos;)) &amp;&amp; 
Account.RecordType.DeveloperName &lt;&gt; &apos;Customer_Sold_To&apos;</errorConditionFormula>
        <errorMessage>Opportunities can only be set to &apos;Closed Won&apos; or &apos;Closing&apos; on Sold To Accounts</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LostReason</fullName>
        <active>true</active>
        <description>Ensure a reason is captured before opportunity is set to lost.</description>
        <errorConditionFormula>AND ( 
$Setup.Global_Bypass_Processes_Settings__c.Run_Validation_Rules__c =True,
ISPICKVAL( StageName, &quot;Lost&quot; ), 
ISBLANK( TEXT(  Loss_Reason__c  ) )
 )</errorConditionFormula>
        <errorDisplayField>Loss_Reason__c</errorDisplayField>
        <errorMessage>Please provide a reason why this opportunity is lost.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpportunitiesOnlyOnProspectAndSoldTo</fullName>
        <active>true</active>
        <description>Opportunities can be created only on Prospect and Sold-To</description>
        <errorConditionFormula>IF(
$Setup.Global_Bypass_Processes_Settings__c.Run_Validation_Rules__c &amp;&amp; 
Account.RecordType.DeveloperName &lt;&gt; &apos;Customer_Sold_To&apos; &amp;&amp; 
Account.RecordType.DeveloperName &lt;&gt; &apos;Customer_Prospect&apos;, 
true,
false)</errorConditionFormula>
        <errorMessage>Opportunities can only be created on &apos;Sold To&apos; And &apos;Prospect&apos; Accounts</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpportunityMustHaveLOB</fullName>
        <active>true</active>
        <description>Validates that an Opportunity should always have an LOB. Note that if the Opportunity&apos;s LOB field is left empty, it is automatically taken over from the Account.</description>
        <errorConditionFormula>ISBLANK(TEXT(LOB1__c)) &amp;&amp; ISBLANK(TEXT(Account.Primary_LOB1__c))</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>Specify this Opportunity&apos;s LOB, either on this record or on the parent Account</errorMessage>
    </validationRules>
</CustomObject>