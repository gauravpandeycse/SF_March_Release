    <fields>
        <fullName>AmountNoDec__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT ( CurrencyIsoCode ) + &apos; &apos; +
IF( Amount &gt;= 1000000000,
TEXT( ROUND( Amount/1000000000,2 ) ) +&apos;B&apos;,
IF( Amount &gt;= 1000000,
TEXT( ROUND ( Amount/1000000,2 ) ) + &apos;M&apos;,
IF( Amount &gt;= 1000,
TEXT( ROUND( Amount/1000,1 ) ) + &apos;K&apos;, TEXT(Round(Amount,0)))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Potential Annual Net Turnover</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
