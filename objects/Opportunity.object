    <fields>
        <fullName>BO_Type_Display__c</fullName>
        <description>Formula field to display the value of the BO Type picklist.  Created so that WebMerge can correctly display the BO Type value on the Product Report.</description>
        <externalId>false</externalId>
        <formula>CASE(
TEXT(Type), 
&quot;Development__c&quot;, &quot;Development&quot;, 
&quot;Replacement__c&quot;, &quot;Replacement&quot;, 
TEXT(Type)
)</formula>
        <label>BO Type Display</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
