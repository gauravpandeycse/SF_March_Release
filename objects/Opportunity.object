        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>BgcPlasticsId__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <description>SFDP-1131. Carried-over from the Account. Modifiable by user on the Opportunity.</description>
        <externalId>false</externalId>
        <inlineHelpText>Opportunities for the Advanced Materials LOB can have a customer business group classification specified. This is optional. If available on the account the value is carried-over, but can be changed on the opportunity.</inlineHelpText>
        <label>BGC Advanced Materials</label>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>1 AND 2</booleanFilter>
            <errorMessage>You are only allowed to select a customer business group classification for the LOB Advanced Materials</errorMessage>
            <filterItems>
                <field>BusinessGroupClassification__c.Lob1__c</field>
                <operation>equals</operation>
                <value>Advanced Materials</value>
            </filterItems>
            <filterItems>
                <field>BusinessGroupClassification__c.Type__c</field>
                <operation>equals</operation>
                <value>Business Group (Customer)</value>
            </filterItems>
            <infoMessage>Select a valid Customer business group classification for Advanced Materials.</infoMessage>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>BusinessGroupClassification__c</referenceTo>
        <relationshipLabel>Opportunities</relationshipLabel>
        <relationshipName>Opportunities</relationshipName>
        <required>false</required>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
