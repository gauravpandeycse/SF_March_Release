    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>CannotChangeLobValues</fullName>
        <active>true</active>
        <description>Enforces that a user cannot change the LOB1/2/3 fields of an existing LOB Classification. This  has an impact on how the programmatic sharing access is provided. Do not deactivate it.</description>
        <errorConditionFormula>$Setup.Global_Bypass_Processes_Settings__c.Run_Validation_Rules__c =True &amp;&amp;
NOT ( $Permission.LockedAccountBypass ) &amp;&amp;

(NOT(IsNew()) &amp;&amp;
(ISCHANGED(LOB1__c) || ISCHANGED(LOB2__c) || ISCHANGED(LOB3__c)) 
)</errorConditionFormula>
        <errorMessage>You cannot change the LOB1/2/3 values. Remove this classification and create a new one</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LockedPrimaryLOB</fullName>
        <active>true</active>
        <description>Primary LOB is mastered in ERP for Customers, Principals and Suppliers,</description>
        <errorConditionFormula>AND ( 
$Setup.Global_Bypass_Processes_Settings__c.Run_Validation_Rules__c, 
NOT ( $Permission.LockedAccountBypass ), 
Account__r.LockedForIntegration__c, 
Primary__c
)</errorConditionFormula>
        <errorMessage>You are not allowed to change the primary LOB on customers. You can edit this information in your ERP system</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>